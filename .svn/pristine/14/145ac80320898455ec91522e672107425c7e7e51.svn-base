@charset "utf-8";
/* CSS Document */
body{
/*	background-color:#F8F7DF;	
*/	font-family: "Microsoft Yahei";
	}
/**----------------------login-------------------**/
.login{
	width:850px;
	height:680px;
	background-image:url(../images/login_bg.jpg);
	margin:100px 0px 0px 248px;
	border-radius:7px;
	box-shadow:4px 5px 4px #666,-4px -3px 4px #666;
	display:inline-block;
	}
.login .texta{
	width:460px;
	height:241px;
	margin:44px auto auto 173px;
	padding:60px 0 0 50px;
	}
.head span{
	position:absolute;
	margin-top:40px;
	margin-left: 160px;
}
.texta span{
	color:#027bcc;
	font-size:23px;
	margin:9px 11px 0 0;
	display:inline-block;
	}
.texta .inputin, .content .center .inputin{
	width:270px;
	border:solid 2px #999999;
	height:23px;
	}
.choose{
	width:270px;
	color:#027bcc;
	font-size:16px;
	margin:25px 0 0 22px;
	}
.texta .end{
	width:200px;
	margin:25px 0 0 227px;
	}
.texta .end input, .center .bot input {
	background-color:#027bcc;
	color:#FFF;
	font-size:20px;
	border:none;
	margin-right:10px;
	
	}
	
/**----------------------personal_information---------------------------------***/

.content{
	width:1050px;
	height:auto;
	margin:5px 0 0 136px;

	}
.content .head{
	height:120px;
	background-color:#4876FF;
	box-shadow:0 1px 1px #666;
	}
.content .main{
	height:auto;
	margin-top:0px;
	border-radius:13px;
	}
.content .center .ul1{
	padding-left:0px;
	width:70%;
	float:left;
	}
.content .center li{
	font-size:15px;
	border: solid 1px #999999; 
	list-style:none;
	margin-top:1px;
	/*border-bottom:dashed 1px #666;*/
	}
.content .center .ul2{
	padding-left:0px;
	width:30%;
	float:left;
	}
.content .center li span{
	font-size:15px;
	color:black;
	margin:3px 0 0 8px;
	}
.title{
    border-bottom:2px dotted  #4876FF;
    width:100%;
    color:black;
    font-size:21px;
    height:31px;
    
}
.title span{
    margin-bottom:2px;
}
/*.main .photo{
	width:150px;
	height:184px;
	background-color:#0F3;
	margin-bottom:5px;
	float:left;
	box-shadow:1px 1px 1px #999;
	}*/
.main .infor{
	width:685px;
	height:180px;
	float:left;
	margin:3px 0 2px 0;
	border:solid 1px #4876FF;
	padding:10px;
	color:black;
	}
.infor .left1{
	width:63%;
	float:left;
	margin-top:0px;
	}
.infor .left1 span{
	display:block;
	margin-bottom:2px;
	font-size:20px;
	width:700px;
	}
.infor img{
	float:left;
	margin:35px 0 0 45px;
	display:inline;
	}
.main .sure{
	width:306px;
	height:180px;
	float:left;
	margin:3px 0 5px 15px;
	border:solid 1px #4876FF;
	padding:10px;
	color:black;
	border-radius:5px;
	}
.sure p{
	font-size:17px;
	color:black;
	display:inline;
	}
.content .center{
	height:auto;
	width:1027px;
	border:solid 1px #4876FF;
	padding:10px;
	color:black;
	display:inline-block;
	}
.content .center li{
	font-size:15px;
	list-style:none;
	/*border-bottom:dashed 1px #666;*/
	}
.content .center li span{
	font-size:15px;
	color:black;
	margin:3px 0 0 8px;
	}
.content .center .btn{
	float:right;
	/*background-color:#027bcc;*/
	color:black;
	font-size:16px;
	width:85px;
	height:22px;
	border:none;
	margin-right:5px;
	box-shadow:1px 1px 1px #666;
	}
.content .center .bot{
	height:auto;
	border-top:dotted 2px #4876FF;
	color:black;
	margin-top:5px;
	padding-top:5px;
	}
.content .center .topp{
	height:auto;
	border-bottom:dotted 2px #4876FF;
	color:black;
	margin-top:5px;
	margin-bottom:5px;
	padding:3px;
	font-size:20px;
	}

.content .footer{
	margin-top:1px;
	height:120px;
	background-color:#4876FF;
	box-shadow:0 1px 1px #666;
	}